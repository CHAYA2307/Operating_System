#include <stdio.h>

int main() {
    int mat[10][10], i, j, n, sum = 0;
    printf("Enter size of square matrix (n x n): ");
    scanf("%d", &n);
    printf("Enter elements:\n");
    for(i = 0; i < n; i++) {
        for(j = 0; j < n; j++) {
            scanf("%d", &mat[i][j]);
            if(i == j) sum += mat[i][j];
        }
    }
    printf("Sum of left diagonal: %d", sum);
    return 0;
}
